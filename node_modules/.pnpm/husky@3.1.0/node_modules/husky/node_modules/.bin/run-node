#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ckc/Desktop/yinke/ckcs-lint/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ckc/Desktop/yinke/ckcs-lint/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../../../../../run-node@1.0.0/node_modules/run-node/run-node" "$@"
else
  exec bash  "$basedir/../../../../../run-node@1.0.0/node_modules/run-node/run-node" "$@"
fi
